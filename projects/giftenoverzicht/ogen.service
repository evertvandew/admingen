[Unit]
Description=Start Overzicht Generator server

[Service]
Type=simple
User=ogen
Group=ogen
ExecStart=/home/ogen/.virtualenv/bin/python3.6 /home/ogen/giftenoverzicht/run giftenoverzicht
WorkingDirectory=/home/ogen/giftenoverzicht
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
